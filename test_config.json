{
  "router": {
    "ip": "127.0.0.1:8080",
    "password": "test_password",
    "host": "test_router",
    "timeout": 10
  },
  "server": {
    "port": 9001,
    "metrics_path": "/metrics",
    "namespace": "miwifi",
    "read_timeout": "30s",
    "write_timeout": "30s",
    "idle_timeout": "60s"
  },
  "cache": {
    "enabled": true,
    "ttl": "30s"
  },
  "logging": {
    "level": "info",
    "format": "text"
  },
  "memory": {
    "enabled": true,
    "optimize_on_collect": true,
    "force_gc_on_close": true,
    "track_allocations": true,
    "enable_pool_stats": true
  }
}